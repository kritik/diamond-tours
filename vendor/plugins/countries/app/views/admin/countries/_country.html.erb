<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(country) -%>">
  <span class='title'>
    <%=h country.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), country_url(country),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_country_path(country),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_country_path(country),
        :class => "cancel confirm-delete",
        :title => t('.delete') %>
  </span>
</li>
