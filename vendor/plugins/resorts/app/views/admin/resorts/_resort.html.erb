<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(resort) -%>">
  <span class='title'>
    <%=h resort.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), resort_url(resort),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_resort_path(resort),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_resort_path(resort),
        :class => "cancel confirm-delete",
        :title => t('.delete') %>
  </span>
</li>
