<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(travel) -%>">
  <span class='title'>
    <%=h travel.hotel.resort.title %> <%=h travel.hotel.title %> <%=h travel.id %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), travel_url(travel),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_travel_path(travel),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_travel_path(travel),
        :class => "cancel confirm-delete",
        :title => t('.delete') %>
  </span>
</li>
