<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(tour_type) -%>">
  <span class='title'>
    <%=h tour_type.name %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), tour_type_url(tour_type),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_tour_type_path(tour_type),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_tour_type_path(tour_type),
        :class => "cancel confirm-delete",
        :title => t('.delete') %>
  </span>
</li>
